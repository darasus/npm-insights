{
  "name": "npm-insights",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=web",
    "lint": "turbo run lint",
    "generate": "turbo run generate --filter=generator",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.12",
    "@types/react": "18.0.17",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest",
    "typescript": "^4.8.4"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.3.5",
    "@mdx-js/react": "^2.1.5",
    "ms": "^2.1.3",
    "next": "12.3.1",
    "next-mdx-remote": "^4.1.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "zod": "^3.19.1"
  },
  "packageManager": "pnpm@7.13.1"
}
