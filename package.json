{
  "name": "npm-insights",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=web",
    "lint": "turbo run lint",
    "generate": "turbo run generate --filter=generator",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@types/ms": "^0.7.31",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^6.0.0",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "ms": "^2.1.3"
  },
  "packageManager": "pnpm@7.13.1"
}