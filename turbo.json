{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "REDIS_URL",
    "VERCEL_URL",
    "PORT",
    "VERCEL_ENV",
    "NEXT_PUBLIC_VERCEL_ENV",
    "NEXT_PUBLIC_GA_TRACKING_ID",
    "NEXT_PUBLIC_VERCEL_URL",
    "NODE_ENV"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "cache": false
    }
  }
}
