{
  "$schema": "https://turborepo.org/schema.json",
  "globalEnv": [
    "REDIS_URL",
    "VERCEL_URL",
    "PORT",
    "VERCEL_ENV",
    "NEXT_PUBLIC_VERCEL_ENV"
  ],
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "cache": false,
      "env": [
        "REDIS_URL",
        "VERCEL_URL",
        "PORT",
        "VERCEL_ENV",
        "NEXT_PUBLIC_VERCEL_ENV"
      ],
      "dependsOn": []
    }
  }
}
