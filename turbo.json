{
  "$schema": "https://turborepo.org/schema.json",
  "globalDependencies": ["$REDIS_URL","$VERCEL_URL", "$PORT"],
  "globalEnv": [
    "REDIS_URL"
  ],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "generate": {
      "cache": false,
      "env": [
        "REDIS_URL",
        "VERCEL_URL",
        "PORT"
      ]
    }
  }
}
